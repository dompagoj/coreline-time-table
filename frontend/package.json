{
  "name": "coreline-time-table",
  "version": "0.1.0",
  "description": "Coreline time table",
  "author": "Domagoj",
  "homepage": "./",
  "private": true,
  "scripts": {
    "dev": "react-scripts-ts start",
    "start": "concurrently \"BROWSER=none npm run dev\" \"wait-on http://localhost:3000 && electron ./electron-app/index.js\"",
    "compile-electron-app": "tsc electron-app/index.ts",
    "build": "react-scripts-ts build",
    "pack": "electron-builder --dir",
    "dist": "electron-builder",
    "eject": "react-scripts-ts eject"
  },
  "dependencies": {
    "antd": "^3.10.0",
    "axios": "^0.18.0",
    "concurrently": "^4.0.1",
    "electron-is-dev": "^1.0.1",
    "emotion": "^9.2.12",
    "formik": "^1.3.1",
    "mobx": "^5.5.0",
    "mobx-react": "^5.2.8",
    "mobx-react-router": "^4.0.5",
    "moment": "^2.22.2",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "react-google-login": "^3.2.1",
    "react-media": "^1.8.0",
    "react-router-dom": "^4.3.1",
    "react-scripts-ts": "3.1.0",
    "ts-node": "^7.0.1",
    "wait-on": "^3.1.0"
  },
  "devDependencies": {
    "@types/axios": "^0.14.0",
    "@types/electron-is-dev": "^0.3.0",
    "@types/node": "^10.11.4",
    "@types/react": "^16.4.15",
    "@types/react-dom": "^16.0.8",
    "@types/react-router-dom": "^4.3.1",
    "electron": "^3.0.2",
    "electron-builder": "^20.28.4",
    "electron-devtools-installer": "^2.2.4",
    "tslint-eslint-rules": "5.4.0",
    "tslint-react": "^3.6.0",
    "typescript": "^3.1.1"
  },
  "main": "public/main.js",
  "build": {
    "appId": "com.your-domain",
    "compression": "normal",
    "productName": "coreline-time-table",
    "directories": {
      "buildResources": "build",
      "output": "dist"
    },
    "mac": {
      "icon": "icons/icon.icns",
      "type": "distribution",
      "target": [
        "pkg",
        "dmg",
        "mas"
      ]
    }
  }
}
