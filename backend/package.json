{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "start": "NODE_ENV=production node .build/src/server/app.js",
    "start-pm2": "pm2 start .build/src/server --name ctt",
    "build": "tsc",
    "restart": "pm2 restart ctt",
    "dev": "ts-node-dev --respawn --type-check --transpileOnly ./src/server/app.ts",
    "lint": "tslint --fix \"./src/**/*.ts*\"",
    "db:start": "docker container start 5f5accb6fddd",
    "db:init": "docker-compose -f docker-compose.yml up -d",
    "migration:generate": "typeorm migration:generate --dir src/data/migrations -n",
    "migrate:up": "typeorm migration:run",
    "migrate:revert": "typeorm migration:revert"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "google-auth-library": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "node-schedule": "^1.3.0",
    "pg": "7.5.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "0.2.12"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.117",
    "@types/morgan": "^1.7.35",
    "@types/node": "^8.0.29",
    "nodemon": "^1.18.4",
    "ts-node-dev": "^1.0.0-pre.32",
    "tslint": "^5.11.0",
    "tslint-eslint-rules": "^5.4.0",
    "typescript": "^3.1.3"
  }
}
