{
  "name": "server",
  "version": "1.0.0",
  "description": "",
  "main": "src/index.js",
  "engines": {
    "node": "10.x.x"
  },
  "scripts": {
    "start": "NODE_ENV=production node .build/src/server/app.js",
    "start-pm2": "pm2 start .build/src/server --name ctt",
    "build": "tsc",
    "restart": "pm2 restart ctt",
    "dev": "ts-node-dev --respawn --type-check --transpileOnly ./src/server/app.ts",
    "lint": "eslint --ext .ts src/**",
    "lint:fix": "eslint --ext .ts --fix src/**",
    "db:start": "docker container start 5f5accb6fddd",
    "db:init": "docker-compose -f docker-compose.yml up -d",
    "migration:generate": "typeorm migration:generate --dir src/data/migrations -n",
    "migration:create": "typeorm migration:create -n",
    "migrate:up": "typeorm migration:run",
    "migrate:revert": "typeorm migration:revert"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@google-cloud/storage": "^2.5.0",
    "app-root-path": "^2.2.1",
    "body-parser": "^1.18.3",
    "class-validator": "^0.9.1",
    "cors": "^2.8.5",
    "dotenv": "^6.1.0",
    "express": "^4.16.4",
    "google-auth-library": "^2.0.0",
    "jsonwebtoken": "^8.3.0",
    "lodash": "^4.17.11",
    "moment": "^2.22.2",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "node-schedule": "^1.3.0",
    "pg": "7.5.0",
    "reflect-metadata": "^0.1.12",
    "typeorm": "0.2.12"
  },
  "devDependencies": {
    "@types/app-root-path": "^1.2.4",
    "@types/express": "^4.16.0",
    "@types/jsonwebtoken": "^8.3.0",
    "@types/lodash": "^4.14.117",
    "@types/morgan": "^1.7.35",
    "@types/multer": "^1.3.7",
    "@types/node": "^8.0.29",
    "@typescript-eslint/eslint-plugin": "^1.6.0",
    "@typescript-eslint/parser": "^1.6.0",
    "eslint": "^5.16.0",
    "nodemon": "^1.18.4",
    "ts-node-dev": "^1.0.0-pre.32",
    "typescript": "^3.4.1"
  }
}
